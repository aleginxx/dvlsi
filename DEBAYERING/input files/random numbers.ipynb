    "import random\n",
    "\n",
    "# Generate 1048576 random numbers and write them to a file\n",
    "with open(\"bayer1024x1024.txt\", \"w\") as file:\n",
    "    for _ in range(1048576):\n",
    "        random_number = random.randint(0, 255)\n",
    "        file.write(str(random_number) + \"\\n\")\n"
